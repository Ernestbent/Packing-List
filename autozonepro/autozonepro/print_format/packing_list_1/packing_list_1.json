 "html": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n<meta charset=\"UTF-8\">\n<title>Packing List</title>\n<link href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css\" rel=\"stylesheet\">\n<style>\n    *, *::before, *::after { box-sizing: border-box; }\n    body { font-family: 'Segoe UI', Arial, sans-serif; background:#fff; margin:0; padding:0; }\n    .print-container { width: 100%; max-width: 900px; margin: 0 auto; padding: 10px 20px; overflow: hidden; }\n    .card { border:none; border-radius:8px; box-shadow:0 1px 4px rgba(0,0,0,0.05); }\n    .card-header { font-weight:600; border-bottom:1px solid #eee; padding:8px 0; background:#f8f9fa; }\n    .table { table-layout: fixed; width: 100%; word-wrap: break-word; }\n    .table th, .table td { font-size: 0.90rem; padding: 4px 6px; vertical-align: middle; }\n    .weight-once { font-weight:600; color:#155724; }\n    .signature { border-top:1px solid #aaa; padding-top:6px; text-align:center; margin-top:25px; font-size:0.9rem; }\n    .fw-bold { font-weight: 600 !important; }\n    .box-green  { background-color: #d4edda !important; }\n    .box-blue   { background-color: #e3f2fd !important; }\n\n    @media print {\n        body { background:#fff; padding:0; margin:0; }\n        @page { size: A4; margin: 10mm; }\n        .print-container { padding:0; margin-top:5px; overflow: hidden !important; }\n        .page-break { page-break-before: always; }\n        .card { box-shadow:none; border:1px solid #ddd; }\n        -webkit-print-color-adjust: exact !important;\n        color-adjust: exact !important;\n    }\n</style>\n</head>\n<body>\n\n{% set cust = doc.custom_customer %}\n{% set customer = frappe.get_doc(\"Customer\", cust) if cust else none %}\n\n<!-- ORIGINAL COPY -->\n<p class=\"text-end fw-bold\" style=\"font-size:14px;margin:0;margin-top:5px;\">Original Copy</p>\n<div class=\"print-container\">\n    <div class=\"text-center mb-3\">\n        <h5 class=\"mb-1\">Autozone Professional Ltd</h5>\n        <h6 style=\"border-bottom:2px solid #d4af37; display:inline-block; padding-bottom:3px;\">PACKING LIST</h6>\n    </div>\n\n    <div class=\"row g-3 mb-3\">\n        <div class=\"col-6\">\n            <div class=\"card p-2\">\n                <div class=\"card-header\">Packing List Details</div>\n                <div>\n                    <p class=\"mb-1\"><strong>PL No:</strong> {{ doc.name }}</p>\n                    <p class=\"mb-1\"><strong>Pick List:</strong> <span class=\"fw-bold\">{{ doc.custom_pick_list or \"\u2014\" }}</span></p>\n                    <p class=\"mb-1\"><strong>Sales Order:</strong> {{ doc.custom_sales_order or \"\u2014\" }}</p>\n                    <p class=\"mb-1\"><strong>Date:</strong> {{ frappe.utils.format_date(doc.custom_date or doc.posting_date) }}</p>\n                </div>\n            </div>\n        </div>\n        <div class=\"col-6\">\n            <div class=\"card p-2\">\n                <div class=\"card-header\">Customer Details</div>\n                <div>\n                    {% if customer %}\n                    <p class=\"mb-1\"><strong>Name:</strong> {{ customer.customer_name or \"\u2014\" }}</p>\n                    <p class=\"mb-1\"><strong>Contact:</strong> {{ customer.mobile_no or customer.phone or \"\u2014\" }}</p>\n                    <p class=\"mb-1\"><strong>Location:</strong> {{ customer.location or \"\u2014\" }}</p>\n                    <p class=\"mb-1\"><strong>District:</strong> {{ customer.district or \"\u2014\" }}</p>\n                    {% else %}\n                    <p class=\"mb-1 text-muted\">No customer linked</p>\n                    {% endif %}\n                </div>\n            </div>\n        </div>\n    </div>\n\n    <!-- Packer and Verifiers -->\n    <div class=\"card mb-3\">\n        <div class=\"card-body py-2\">\n            <div class=\"row\">\n                <div class=\"col-4\">\n                    <strong>Packer:</strong> <span class=\"text-primary\">{{ doc.custom_packer or \"\u2014\" }}</span>\n                </div>\n                <div class=\"col-4\">\n                    <strong>Verifier 1:</strong> <span class=\"text-primary\">{{ doc.owner or \"\u2014\" }}</span>\n                </div>\n                <div class=\"col-4\">\n                    <strong>Verifier 2:</strong> <span class=\"text-primary\">{{ doc.modified_by if doc.docstatus == 1 else \"\u2014\" }}</span>\n                </div>\n            </div>\n        </div>\n    </div>\n\n    <!-- TABLE -->\n    <div class=\"card mb-3\">\n        <div class=\"card-body p-0\">\n            <table class=\"table table-bordered table-sm mb-0\">\n                <thead class=\"table-light\">\n                    <tr>\n                        <th style=\"width:10%\">Box</th>\n                        <th style=\"width:48%\">Item Name</th>\n                        <th style=\"width:12%\" class=\"text-center\">Qty</th>\n                        <th style=\"width:15%\" class=\"text-center\">Unit</th>\n                        <th style=\"width:15%\" class=\"text-end\">Weight</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    {% set ns = namespace(current_box=none, box_counter=0) %}\n                    {% for pkg in doc.table_hqkk|sort(attribute=\"box_number\") %}\n                        {% set box_sum = doc.custom_box_summary|selectattr(\"box_number\",\"equalto\",pkg.box_number)|first %}\n                        {% set item_name = frappe.db.get_value(\"Item\", pkg.item, \"item_name\") or pkg.item %}\n\n                        {% if pkg.box_number != ns.current_box %}\n                            {% set ns.box_counter = ns.box_counter + 1 %}\n                            {% set bg = \"box-green\" if ns.box_counter % 2 == 1 else \"box-blue\" %}\n                            {% set ns.current_box = pkg.box_number %}\n                        {% endif %}\n\n                        <tr class=\"{{ bg }}\">\n                            {% if loop.first or pkg.box_number != doc.table_hqkk[loop.index0-1].box_number %}\n                                <td class=\"fw-bold\">{{ pkg.box_number }}</td>\n                                <td>{{ pkg.item }} - {{ item_name }}</td>\n                                <td class=\"text-center\">{{ pkg.quantity }}</td>\n                                <td class=\"text-center\">{{ pkg.uom }}</td>\n                                <td class=\"text-end weight-once\">{{ \"%.2f\"|format(box_sum.weight_kg or 0) }} kg</td>\n                            {% else %}\n                                <td></td>\n                                <td>{{ pkg.item }} - {{ item_name }}</td>\n                                <td class=\"text-center\">{{ pkg.quantity }}</td>\n                                <td class=\"text-center\">{{ pkg.uom }}</td>\n                                <td></td>\n                            {% endif %}\n                        </tr>\n                    {% endfor %}\n                </tbody>\n            </table>\n        </div>\n    </div>\n\n    <!-- TOTALS -->\n    <div class=\"card mb-3\">\n        <div class=\"card-body d-flex justify-content-between py-2\">\n            <div><strong>Total Boxes:</strong> <span class=\"text-primary\">{{ doc.custom_box_summary|length }}</span></div>\n            <div><strong>Total Weight:</strong> <span class=\"text-primary\">{{ \"%.2f\"|format(doc.custom_box_summary|sum(attribute=\"weight_kg\") or 0) }} kg</span></div>\n        </div>\n    </div>\n\n    <div class=\"row\">\n        <div class=\"col-6\"><div class=\"signature\">____________________<br><small class=\"text-muted\">Prepared By</small></div></div>\n        <div class=\"col-6\"><div class=\"signature\">____________________<br><small class=\"text-muted\">Received By</small></div></div>\n    </div>\n    <div class=\"text-end small text-muted mt-2\">\n        Printed: {{ frappe.utils.now_datetime().strftime('%d-%m-%Y') }}\n    </div>\n</div>\n\n<!-- PAGE BREAK -->\n<div