div>\n\n    <!-- Packer and Verifiers -->\n    <div class=\"card mb-3\">\n        <div class=\"card-body py-2\">\n            <div class=\"row\">\n                <div class=\"col-4\">\n                    <strong>Packer:</strong> <span class=\"text-primary\">{{ doc.custom_packer or \"\u2014\" }}</span>\n                </div>\n                <div class=\"col-4\">\n                    <strong>Verifier 1:</strong> <span class=\"text-primary\">{{ doc.owner or \"\u2014\" }}</span>\n                </div>\n                <div class=\"col-4\">\n                    <strong>Verifier 2:</strong> <span class=\"text-primary\">{{ doc.modified_by if doc.docstatus == 1 else \"\u2014\" }}</span>\n                </div>\n            </div>\n        </div>\n    </div>\n\n    <!-- TABLE -->\n    <div class=\"card mb-3\">\n        <div class=\"card-body p-0\">\n            <table class=\"table table-bordered table-sm mb-0\">\n                <thead class=\"table-light\">\n                    <tr>\n                        <th style=\"width:10%\">Box</th>\n                        <th style=\"width:48%\">Item Name</th>\n                        <th style=\"width:12%\" class=\"text-center\">Qty</th>\n                        <th style=\"width:15%\" class=\"text-center\">Unit</th>\n                        <th style=\"width:15%\" class=\"text-end\">Weight</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    {% set ns = namespace(current_box=none, box_counter=0) %}\n                    {% for pkg in doc.table_hqkk|sort(attribute=\"box_number\") %}\n                        {% set box_sum = doc.custom_box_summary|selectattr(\"box_number\",\"equalto\",pkg.box_number)|first %}\n                        {% set item_name = frappe.db.get_value(\"Item\", pkg.item, \"item_name\") or pkg.item %}\n\n                        {% if pkg.box_number != ns.current_box %}\n                            {% set ns.box_counter = ns.box_counter + 1 %}\n                            {% set bg = \"box-green\" if ns.box_counter % 2 == 1 else \"box-blue\" %}\n                            {% set ns.current_box = pkg.box_number %}\n                        {% endif %}\n\n                        <tr class=\"{{ bg }}\">\n                            {% if loop.first or pkg.box_number != doc.table_hqkk[loop.index0-1].box_number %}\n                                <td class=\"fw-bold\">{{ pkg.box_number }}</td>\n                                <td>{{ pkg.item }} - {{ item_name }}</td>\n                                <td class=\"text-center\">{{ pkg.quantity }}</td>\n                                <td class=\"text-center\">{{ pkg.uom }}</td>\n                                <td class=\"text-end weight-once\">{{ \"%.2f\"|format(box_sum.weight_kg or 0) }} kg</td>\n                            {% else %}\n                                <td></td>\n                                <td>{{ pkg.item }} - {{ item_name }}</td>\n                                <td class=\"text-center\">{{ pkg.quantity }}</td>\n                                <td class=\"text-center\">{{ pkg.uom }}</td>\n                                <td></td>\n                            {% endif %}\n                        </tr>\n                    {% endfor %}\n                </tbody>\n            </table>\n        </div>\n    </div>\n\n    <!-- TOTALS -->\n    <div class=\"card mb-3\">\n        <div class=\"card-body d-flex justify-content-between py-2\">\n            <div><strong>Total Boxes:</strong> <span class=\"text-primary\">{{ doc.custom_box_summary|length }}</span></div>\n            <div><strong>Total Weight:</strong> <span class=\"text-primary\">{{ \"%.2f\"|format(doc.custom_box_summary|sum(attribute=\"weight_kg\") or 0) }} kg</span></div>\n        </div>\n    </div>\n\n    <div class=\"row\">\n        <div class=\"col-6\"><div class=\"signature\">____________________<br><small class=\"text-muted\">Prepared By</small></div></div>\n        <div class=\"col-6\"><div class=\"signature\">____________________<br><small class=\"text-muted\">Received By</small></div></div>\n    </div>\n    <div class=\"text-end small text-muted mt-2\">\n        Printed: {{ frappe.utils.now_datetime().strftime('%d-%m-%Y') }}\n    </div>\n</div>\n\n</body>\n</html>",
